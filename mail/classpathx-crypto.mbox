+
+   /** Serpent's linear transformation. */
+   private void transform(int[] x) {
+      x[0] = x[0] << 13 | x[0] >>> 19;
+      x[2] = x[2] << 3 | x[2] >>> 29;
+      x[1] = x[1] ^ x[0] ^ x[2];
+      x[3] = x[3] ^ x[2] ^ (x[0] << 3);
+      x[1] = x[1] << 1 | x[1] >>> 31;
+      x[3] = x[3] << 7 | x[3] >>> 25;
+      x[0] = x[0] ^ x[1] ^ x[3];
+      x[2] = x[2] ^ x[3] ^ (x[1] << 7);
+      x[0] = x[0] << 5 | x[0] >>> 27;
+      x[2] = x[2] << 22 | x[2] >>> 10;
+   }
+
+   /**
+    * The inverse linear transformation. The XOR with the key is done
+    * here to save space above.
+    *
+    * @param x The current block being decrypted.
+    * @param key The session key.
+    * @param off The index in the key to start from.
+    */
+   private void transformInv(int[] x, int[] key, int off) {
+      x[0] ^= key[off++];
+      x[1] ^= key[off++];
+      x[2] ^= key[off++];
+      x[3] ^= key[off++];
+
+      x[2] = x[2] >>> 22 | x[2] << 10;
+      x[0] = x[0] >>> 5 | x[0] << 27;
+      x[2] = x[2] ^ x[3] ^ (x[1] << 7);
+      x[0] = x[0] ^ x[1] ^ x[3];
+      x[3] = x[3] >>> 7 | x[3] << 25;
+      x[1] = x[1] >>> 1 | x[1] << 31;
+      x[3] = x[3] ^ x[2] ^ (x[0] << 3);
+      x[1] = x[1] ^ x[0] ^ x[2];
+      x[2] = x[2] >>> 3 | x[2] << 29;
+      x[0] = x[0] >>> 13 | x[0] << 19;
+   }
