+      int r4 = r2;
+      r2 = r2 & r3 ^ r1;
+      r1 = (r1 | r3) & r0;
+      r4 = r4 ^ r2 ^ r1;
+      r1 &= r2;
+      r0 = ~r0;
+      r3 ^= r4;
+      r1 ^= r3;
+      r3 = r3 & r0 ^ r2;
+      r0 ^= r1;
+      r3 ^= r0;
+      w[off  ] = r0;
+      w[off+1] = r3 ^ r0;
+      w[off+2] = (r2 & r0 ^ r4 | r3) ^ r1;
