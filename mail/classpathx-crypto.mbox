+   /** Inverse of S-Box 6. */
+   private void sboxI6(int r0, int r1, int r2, int r3, int[] w, int off) {
+      int r4 = r2;
+      r0 ^= r2;
+      r2 &= r0;
+      r4 ^= r3;
+      r3 ^= r1;
+      r2 = ~r2 ^ r3;
+      r4 |= r0;
+      r0 ^= r2;
+      r3 ^= r4;
+      r4 ^= r1;
+      r1 = r1 & r3 ^ r0;
+      r0 = r0 ^ r3 | r2;
+      w[off  ] = r1;
+      w[off+1] = r2;
+      w[off+2] = r4 ^ r0;
+      w[off+3] = r3 ^ r1;
