+      r2 ^= r1;
+      r3 = (r3 | r0) ^ r2;
+      r4 ^= r3;
+      r2 = (r2 | r1) & r0 ^ r4;
+      r4 = (r4 | r0) ^ r1 ^ r2;
+      r1 = r1 & r2 ^ r3;
+      r3 &= r4;
+      r4 ^= r1;
+      w[off  ] = r1;
+      w[off+1] = ~r4;
+      w[off+2] = r3 ^ r4 ^ r0;
